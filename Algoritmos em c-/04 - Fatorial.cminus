int fatorial(int n){
    int i;
    int resposta;
    resposta = 1;
    i = 1;
    while(i <= n){
        resposta = resposta * i;
        i = i + 1;   
    }
    return resposta;
}

void main(void){
    int n;
    int resposta;

    n = input();
    output(n);

    resposta = fatorial(n);

    output(resposta);

 
}
